@model Electronic_Bank.Models.Transaction

@{
	ViewData["Title"] = "Details";
}



<h1 style="margin:20px">Currency Details</h1>




<body style="background: #f2edf3;">
	<div class="col-md-10  stretch-card text-center" style="margin-left: 104px;">
		<div class="card">
			<div class="card-body">
				<h4 class="card-title text-white">Todo</h4>

				<div class="add-items d-flex">
					<div class="col-3">
						@Html.DisplayNameFor(model => model.WalletID)
					</div>
					<input disabled type="text" class="form-control todo-list-input" placeholder="Client Name " asp-for="WalletID" />

				</div>
				<div class="add-items d-flex">
					<div class="col-3">
						@Html.DisplayNameFor(model => model.Wallets.ClientID)
					</div>
					<input disabled type="text" class="form-control todo-list-input" placeholder="Client Name " asp-for="Wallets.ClientID" />

				</div>
				<div class="add-items d-flex">
					<div class="col-3">     @Html.DisplayNameFor(model => model.TransactionAmount)  </div>

					<input disabled class="form-control todo-list-input" placeholder="Client Address" asp-for="TransactionAmount" />

				</div>
				<div class="add-items d-flex">
					<div class="col-3">     @Html.DisplayNameFor(model => model.TransactionDate)  </div>

					<input disabled class="form-control todo-list-input" placeholder="Client Address" asp-for="TransactionDate" />

				</div>
				<div class="add-items d-flex">
					<div class="col-3">     @Html.DisplayNameFor(model => model.TransactionDescription)  </div>

					<input disabled class="form-control todo-list-input" placeholder="Client Address" asp-for="TransactionDescription" />

				</div>

				<div class="add-items d-flex">
					<div class="col-3">
						@Html.DisplayNameFor(model => model.TransactionType)
					</div>

					<input disabled class="form-control todo-list-input" placeholder="@Html.DisplayFor(model => model.TransactionType)" />
				</div>


				<form asp-action="Delete">
					<input type="hidden" asp-for="TransactionID" />
					<a asp-action="Index" class="btn btn-secondary">Back to List</a>
				</form>
			</div>
		</div>
	</div>
</body>


